        cgsccpInstXUDTSMsgsRcvd      Counter32,
        cgsccpInstLUDTMsgsSent       Counter32,
        cgsccpInstLUDTSMsgsSent      Counter32,
        cgsccpInstLUDTMsgsRcvd       Counter32,
        cgsccpInstLUDTSMsgsRcvd      Counter32,
        cgsccpInstCrToMtp            Counter32,
        cgsccpInstCrefToMtp          Counter32,
        cgsccpInstCrFromMtp          Counter32,

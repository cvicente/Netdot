      ::= { ceSctpAssocEntry 34 }
      
    ceSctpAssocULPDatagramsQueuedHigh OBJECT-TYPE
      SYNTAX         Unsigned32(0..65535) 
      UNITS          "DataGrams"
      MAX-ACCESS     read-only
      STATUS         current
      DESCRIPTION
           "High water mark for data grams waiting to be sent."

      ::= { ceSctpAssocEntry 35 }
      
    --**************************************************************
    -- Association Statistics

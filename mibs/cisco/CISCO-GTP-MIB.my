    ::= { cGtpPathStatisticsHistEntry 45 }

cGtpHistRoamingTrustedPDPs       OBJECT-TYPE
    SYNTAX                       Counter32
    UNITS                        "pdps"
    MAX-ACCESS                   read-only
    STATUS                       current
    DESCRIPTION
        "This object indicates the historical number of roaming
         PDP contexts in a trusted PLMN on a SGSN path."
